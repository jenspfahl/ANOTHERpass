<PreferenceScreen xmlns:app="http://schemas.android.com/apk/res-auto">

    <SwitchPreferenceCompat
        app:key="YAPM/pref:autofill_suggest_everywhere"
        app:summaryOff="@string/pref_autofill_summary_off_suggest"
        app:summaryOn="@string/pref_autofill_summary_on_suggest"
        app:iconSpaceReserved="false"
        app:singleLineTitle="false"
        app:defaultValue="false"
        app:title="@string/autofill_suggest_everywhere" />

    <MultiSelectListPreference
        app:key="YAPM/pref:autofill_exclusion_list"
        app:iconSpaceReserved="false"
        app:singleLineTitle="false"
        app:title="@string/pref_autofill_exclude_apps" />

    <ListPreference
        app:defaultValue="15"
        app:entries="@array/autofill_deactivation_duration_entries"
        app:entryValues="@array/autofill_deactivation_duration_values"
        app:key="YAPM/pref:autofill_deactivation_duration"
        app:title="@string/autofill_deactivation_duration_title"
        app:iconSpaceReserved="false"
        app:singleLineTitle="false"
        app:useSimpleSummaryProvider="true"/>



</PreferenceScreen>